<%= render partial: "layouts/breadcrumb", locals: { jurisdiction_path: current_user.jurisdiction_path, crumbs: [ {value: "Return to Exposure Dashboard", href: '/'}, {value: "Register New Monitoree", href: nil} ] } %>

<%= react_component("enrollment/Enrollment", {
  current_user: current_user,
  patient: @patient,
  propagated_fields: {},
  editMode: false,
  authenticity_token: form_authenticity_token,
  can_add_group: current_user.can_create_patient?,
  has_group_members: false,
  jurisdictionPaths: Hash[current_user.jurisdiction.subtree.pluck(:id, :path).map {|id, path| [id, path]}],
  groupNumbers: @patient.jurisdiction.immediate_patients.where.not(group_number: nil).distinct.pluck(:group_number).sort
  }) %>

<div class="pb-4"></div>