<% content_for :assets do %>
  <%= javascript_packs_with_chunks_tag 'datatables' %>
<% end %>
<%= render partial: 'layouts/breadcrumb', locals: { jurisdiction_path: current_user.jurisdiction_path, crumbs: [{ value: 'Dashboard', href: nil }] } %>
<h1 class="sr-only">Enrolled Monitorees</h1>

<%= react_component('enroller/EnrollerDashboard', {
                      jurisdiction: {
                        id: current_user.jurisdiction_id,
                        path: current_user.jurisdiction[:path]
                      },
                      jurisdiction_paths: @possible_jurisdiction_paths,
                      all_assigned_users: @all_assigned_users,
                      authenticity_token: form_authenticity_token
                    }) %>
